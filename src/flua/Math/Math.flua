			<meta/>
		</function>
		<comment>\SReturns\Sthe\Sabsolute\Svalue\Sof\S#x\S(positive)</comment>
		<function>
			<name>abs</name>
			<parameters>
				<parameter>x</parameter>
			</parameters>
			<code>
				<if-block>
					<if>
						<condition>
							<greater-or-equal>
								<value>x</value>
								<value>0</value>
							</greater-or-equal>
						</condition>
						<code>
							<return>x</return>
						</code>
					</if>
					<else>
						<code>
							<return>
								<negative>
									<value>x</value>
								</negative>
							</return>
						</code>
					</else>
				</if-block>
			</code>
			<meta/>
		</function>
		<comment>\SReturns\Sa\Srandom\Sinteger\Sbetween\Sa\Sand\Sb</comment>
		<function>
			<name>rand</name>
			<parameters>
				<parameter>a</parameter>
				<parameter>b</parameter>
			</parameters>
			<code>
				<ensure>
					<greater-or-equal>
						<value>result</value>
						<value>a</value>
					</greater-or-equal>
					<less-or-equal>
						<value>result</value>
						<value>b</value>
					</less-or-equal>
				</ensure>
				<return>
					<call>
						<function>
							<access>
								<value>flua</value>
								<value>rand</value>
							</access>
						</function>
						<parameters>
							<parameter>a</parameter>
							<parameter>b</parameter>
						</parameters>
					</call>
				</return>
			</code>
		</function>
		<comment>\SReturns\Sa\Srandom\SFloat\Sbetween\Sa\Sand\Sb</comment>
