				</ensure>
				<return>
					<call>
						<function>
							<access>
								<value>flua</value>
								<value>randFloat</value>
							</access>
						</function>
						<parameters>
							<parameter>a</parameter>
							<parameter>b</parameter>
						</parameters>
					</call>
				</return>
