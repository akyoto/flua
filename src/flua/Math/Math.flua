		<comment>\SReturns\Sthe\Ssquare\Sroot\Sof\S#x</comment>
		<function>
			<name>sqr</name>
			<parameters>
				<parameter>x</parameter>
			</parameters>
			<code>
				<require>
					<greater-or-equal>
						<value>x</value>
						<value>0</value>
					</greater-or-equal>
				</require>
				<return>
					<call>
						<function>
							<access>
								<value>flua</value>
								<value>sqrt</value>
							</access>
						</function>
						<parameters>
							<parameter>x</parameter>
						</parameters>
					</call>
				</return>
			</code>
			<meta/>
		</function>
		<comment>\SReturns\Sln\Sof\S#x</comment>
		<function>
			<name>ln</name>
			<parameters>
				<parameter>x</parameter>
			</parameters>
			<code>
				<return>
					<call>
						<function>
							<access>
								<value>flua</value>
								<value>log</value>
